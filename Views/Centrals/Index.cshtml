@model IEnumerable<Central>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="card-body">
    <div class="card-header">
        <div class="row">
            <div class="col-sm-12"> <h4>السنترال</h4></div>
        </div>
    </div>

    <form asp-action="index">
        <div class="row">
            <div class="col-md-1">
                <label>Search</label>
            </div>
            <div class="col-md-4">
                <input name="id" type="text" />
            </div>
            <div class="col-md-2">
                <input asp-action="index" class="btn-sm btn-info" value="Search" type="submit" />
            </div>
        </div>

    </form>


    <p>
        <a asp-action="Create">Create New</a>
    </p>

    <div class="table-responsive">
        <table id="tblExport" class="table  table-bordered table-sm">
            <thead>
                <tr class="table-info">
                    <th>
                        @Html.DisplayNameFor(model => model.aDAF_Branches)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LandlineNumber1)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LandlineNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.MobileNo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AffiliateCentral)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.InternetBillValue)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Subscriptiondate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Thebunch)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CorrectTheSituation)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.FromCts)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ToCts)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Note)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.aDAF_Branches.branch_id)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LandlineNumber1)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LandlineNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MobileNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AffiliateCentral)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.InternetBillValue)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Subscriptiondate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Thebunch)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CorrectTheSituation)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FromCts)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ToCts)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Note)
                        </td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.ID_central">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.ID_central">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.ID_central">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="row">
        <div class="col-sm-4">
            <input type="button" onclick="tableToExcel('tblExport', 'W3C Example Table')" value="Export to Excel">
        </div>
    </div>
</div>
